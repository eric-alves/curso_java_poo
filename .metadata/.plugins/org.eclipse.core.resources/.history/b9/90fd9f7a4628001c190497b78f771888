package entities;

public class conta {
	
	Integer numero;
	String nome;
	Float saldo;

	public conta(Integer numero, String nome, Float deposito) {
		// TODO Auto-generated constructor stub
		this.numero = numero;
		this.nome = nome;
		this.saldo = deposito;
	}
	
	public conta(Integer numero, String nome) {
		// TODO Auto-generated constructor stub
		this.numero = numero;
		this.nome = nome;
		this.saldo = (float) 0;
	}

}
