package entities;

public class conta {
	
	private int numero;
	private String nome;
	private float saldo;

	public conta(Integer numero, String nome, Float deposito) {
		// TODO Auto-generated constructor stub
		this.numero = numero;
		this.nome = nome;
		this.saldo = deposito;
	}
	
	public conta(int numero, String nome) {
		// TODO Auto-generated constructor stub
		this.numero = numero;
		this.nome = nome;
		this.saldo = (float) 0;
	}


}
